---
layout: null
regenerate: true
---
{% assign first = true %}
[
{% for item in site.recipes %}{% if item.title != null and item.title != empty %}{% unless first %},{% endunless %}{
    "title": {{ item.title | jsonify }},
    "data_search": ".r_{{ item.slug }}",
    "ingredients": "{{ item.ingredients | strip_html | replace: '
', ', ' | remove:'"' }}",
    "categories": "{% for category in item.categories %}{{ category }}{% unless forloop.last %}, {% endunless %}{% endfor %}",
    "technique": "{{ item.technique }}"
}
{% assign first = false %}{% endif %}{% endfor %}]